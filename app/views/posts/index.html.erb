<h3>スケジュール一覧</h3>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>タイトル</th>
      <th>開始日</th>
      <th>終了日</th>
      <th>終日</th>
      <th>スケジュール更新日時</th>
      <th>確認</th>
      <th>編集</th>
      <th>削除</th>
    </tr>
  </thead>
  
  <tbody>
    <% @posts.each do |post| %>
    <tr>
      <td><%= post.id %></td>
      <td><%= post.title %></td>
      <td><%= post.satrtDay %> </td>
      <td><%= post.endDay %> </td>
      <td><%= post.allDay %></td>
      <td><%= post.updated_at %></td>
    </tr>
    <% end %>
  </tbody>
</table>

<%= link_to "スケジュール新規登録", :new_post %>

